{
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-patch": "npm version patch && npm publish --access=public"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ibrunoss/react-assets",
  "author": "Bruno Silva Santana",
  "module": "dist/react-assets.esm.js",
  "size-limit": [
    {
      "path": "dist/react-assets.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-assets.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addons": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "babel-loader": "^9.1.3",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^11.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  }
}